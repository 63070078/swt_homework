pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'Alice', description: 'Enter your name')
        string(name: 'Ages', defaultValue: '21', description: 'Enter your age')
    }
    stages {
        stage('Example') {
            steps {
                echo "Hello from Slave ${params.NAME} of ${params.Ages}"
                
            }
        }
         stage('checkout'){
        steps{
                git 'https://github.com/63070078/SOFTWARE-DEVELOPMENT-TOOLS-AND-ENVIRONMENTS/blob/main/Week7/'
            }
        }
        stage('Run Python'){
        steps{
                sh 'python /Homework/myscript.py'
            }
        }
    }
}
